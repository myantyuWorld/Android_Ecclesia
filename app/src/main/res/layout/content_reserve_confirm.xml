<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_reserve_confirm"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.yuichi_oba.ecclesia.activity.ReserveConfirmActivity"
    tools:showIn="@layout/app_bar_reserve_confirm">

    <view
        android:id="@+id/arconfirm_view_confirm"
        class="com.example.yuichi_oba.ecclesia.view.Reseve_confirmView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    </view>

    <Button
        android:id="@+id/arconfirm_btn_correct"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="7dp"
        android:onClick="onClickKakutei"
        android:text="確定" />

    <Button
        android:id="@+id/arconfirm_btn_mem_confirm"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/arconfirm_btn_correct"
        android:layout_marginStart="60dp"
        android:layout_marginTop="198dp"
        android:onClick="onClickMemConfirm"
        android:text="参加者表示" />
</RelativeLayout>
